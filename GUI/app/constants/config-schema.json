{
  "configured": {
    "type": "boolean",
    "default": false
  },
  "local": {
    "type": "boolean",
    "default": true
  },
  "apiProtocol": {
    "type": "string",
    "default": "http",
    "enum": ["http", "https"]
  },
  "apiHostname": {
    "type": "string",
    "default": "localhost"
  },
  "apiPort": {
    "type": "integer",
    "default": 9898,
    "exclusiveMinimum": 0,
    "maximum": 65535
  },
  "apiPath": {
    "type": "string",
    "default": "/api/"
  },
  "publicPath": {
    "type": "string",
    "default": "/storage/"
  },
  "dataPath": {
    "type": "string"
  },
  "dockerExecutablePath": {
    "type": "string",
    "default": "docker"
  },
  "containerName": {
    "type": "string",
    "default": "RNADetector"
  },
  "apiKey": {
    "type": "string",
    "default": ""
  }
}
